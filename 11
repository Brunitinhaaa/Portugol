Algoritmo "Tinta necessaria para pintar parede"

Var
  Lar, Alt, A, T: real
  // Largura,Altura,Área e Litros de tinta
Inicio
 Escreva ("Qual a largura da parede a ser pintada?")
 Leia (Lar)
 Escreva ("E Qual a altura?")
 Leia (Alt)
 A <- Lar * Alt
 T <- A * 2
 Escreva ("Para uma área de ",A," m², são necessários ",T," litros de tinta")

Fimalgoritmo